#include <bits/stdc++.h>
using namespace std;
typedef long long ll ;
#define all(v) (v.begin(),v.end())
typedef vector<ll> vi;


int main()
{
    ll n,sum=0;
    cin>>n;
    ll arr1[n];
    ll arr2[n];
    
    
    for(ll i=0; i<n; i++){
        cin>>arr1[i]>>arr2[i];
    }
    for(ll i=0; i<n; i++){
        for(ll j=0; j<n; j++){
            if(arr1[i]==arr2[j]){sum++;}
        }
    }
    cout<<sum<<endl;


    

    	return 0;
}
