#include <bits/stdc++.h>
using namespace std;
typedef long long ll ;
#define all(v) (v.begin(),v.end())
typedef vector<ll> vi;


int main()
{
    ll sum=0;
    string s;
    ll a[4];
    cin>>a[0]>>a[1]>>a[2]>>a[3];
    cin>>s;
    for(ll i=0; i<s.size() ;i++){
        sum+=a[((int)s[i]-(int)'0')-1];
    }
    cout<<sum<<endl;
	return 0;
}
