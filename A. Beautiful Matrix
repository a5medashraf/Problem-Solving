#include <bits/stdc++.h>
using namespace std;

int main() {
    int x=0,a[5],b[5],c[5],d[5],e[5];
    for(int i=0;i<5;i++){
        a[i]=0;
        b[i]=0;
        c[i]=0;
        d[i]=0;
        e[i]=0;
    }
    for(int i=0;i<5;i++){
        cin>>a[i]>>b[i]>>c[i]>>d[i]>>e[i];
    }
    for(int i=0;i<5;i++){
        if(a[i]==1){
            x=abs((i+1)-3)+abs((3-1));}
        else if(b[i]==1){
            x=abs((i+1)-3)+abs((3-2));}
        else if(c[i]==1){
            x=abs((i+1)-3)+abs((3-3));}
        else if(d[i]==1){
            x=abs((i+1)-3)+abs((3-4));}
        else if(e[i]==1){
            x=abs((i+1)-3)+abs((3-5));}
    }
    cout<<x<<endl;

    return 0;
}
