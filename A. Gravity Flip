#include <bits/stdc++.h>
using namespace std;
typedef long long ll ;
#define all(v) (v.begin(),v.end())
typedef vector<ll> vi;


int main()
{
    ll n,x;
    cin>>n;
    vi v;
    for(ll i=0; i<n ;i++){
        cin>>x;
        v.push_back(x);
    }
    
    sort(v.begin(),v.end());
    for(auto it : v ){
        cout<<it<<" ";
    }

    
	return 0;
}
